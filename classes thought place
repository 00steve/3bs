input/source:
 - Name()
 - Type()
 	- rotaryEncoder
 	- button
 	- voltageJack
 - ValueRef()
 - ValueMax()
 - ValueMin()
 - ValueType()
 - GetValue()
 - SampleValue()
 
output/destination:
 - Name()
 - Type()
 	- voltageJack
 		- PWM (teensy default pin outputs)
 		- MCP4728 (4 4096step DACs)
 - ValueRef()
 - ValueMax()
 - ValueMin()
 - ValueType()
 - SetValue()
 
