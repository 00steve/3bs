 - typeID
 	- boolean
 	- float
 	- int
 
hardware:
 - Inputs()
 - Outputs()
 


algorithm:
 - Init()
 - Run()
 - Draw()
 - Outputs()
 - Inputs()
 - Reset()
 - GetMenu(string menuName)
 

 	
